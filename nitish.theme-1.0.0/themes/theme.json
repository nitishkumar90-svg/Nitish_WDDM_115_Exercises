{
	"name": "Nitish's Theme",
	"type": "light",
	"colors": {		
		"tab.inactiveBackground": "#E50762",
		"tab.activeBackground": "#057052",
		"tab.activeForeground": "#EEEEEE",
		"sideBar.background": "#D8F29A",
		"sideBar.border": "#D8F29A",
		"sideBar.foreground": "#111111",
		"sideBarSectionHeader.foreground": "#E50762",
		"sideBarSectionHeader.background": "#D8F29A",
		"sideBarTitle.foreground": "#111111",
		"editor.background": "#EFF1E4",
		"editor.foreground": "#057052", 
		"editor.selectionBackground": "#D8F29A",
		"editor.selectionForeground": "#x057052",
		"editor.inactiveSelectionBackground": "#999999",
		"editorIndentGuide.background": "#D2D2D2",
		"extensionButton.prominentBackground": "#E50762",
		"extensionButton.prominentHoverBackground": "#E50762",
		"activityBar.background": "#D8F29A",
		"activityBar.foreground": "#111111",
		"activityBar.border": "#111111",
		"list.hoverBackground": "#057052",
		"list.hoverForeground": "#EEEEEE",
		"list.activeSelectionBackground": "#E50762",
		"list.activeSelectionForeground": "#EEEEEE",
		"errorForeground": "red",
		"__BONUS__": "Using the Visual Studio Code theme documentation, style the 'activity bar'. https://code.visualstudio.com/api/references/theme-color"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#00B274"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#057052"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#a22f29"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#057052"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#00FD0F"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#00FD0F"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#00FD0F"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#038301"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name",
				"support.type",
				"support.class",
				"support.orther.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#E50762"
			}
		}
	]
}
